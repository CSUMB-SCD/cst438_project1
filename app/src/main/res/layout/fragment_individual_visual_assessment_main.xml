<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/vf_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="group_project.csumb.com.autitrak.simone.VisualAssessmentMainFragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >


        <TextView
            android:id="@+id/textview_beginner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="28dp"
            android:text="Beginner"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/beginner_start"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.44"
            android:layout_marginBottom="67dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_intermediate"
            style="@style/TextStyle"/>

        <Button
            android:id="@+id/beginner_start"
            android:layout_width="106dp"
            android:layout_height="37dp"
            android:layout_marginRight="70dp"
            android:text="Start"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="42dp"
            app:layout_constraintBottom_toTopOf="@+id/intermediate_start"
            android:theme="@style/ButtonStyle"/>

        <TextView
            android:id="@+id/textview_intermediate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Intermediate"
            app:layout_constraintRight_toLeftOf="@+id/intermediate_start"
            android:layout_marginRight="49dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="62dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_advanced"
            style="@style/TextStyle"/>

        <Button
            android:id="@+id/intermediate_start"
            android:layout_width="106dp"
            android:layout_height="37dp"
            android:layout_marginRight="70dp"
            android:text="Start"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="44dp"
            app:layout_constraintBottom_toTopOf="@+id/advanced_start"
            android:theme="@style/ButtonStyle"/>

        <TextView
            android:id="@+id/textview_advanced"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Advanced"
            android:layout_marginLeft="33dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="237dp"
            app:layout_constraintRight_toLeftOf="@+id/advanced_start"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.447"
            style="@style/TextStyle"/>

        <Button
            android:id="@+id/advanced_start"
            android:layout_width="106dp"
            android:layout_height="37dp"
            android:text="Start"
            android:layout_marginRight="70dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="226dp"
            android:theme="@style/ButtonStyle"
            />

        <TextView
            android:id="@+id/textview_social_cues"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="29dp"
            android:text="Social Cues Assessment"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textview_beginner"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.125"
            style="@style/TextStyle"/>
    </android.support.constraint.ConstraintLayout>

</FrameLayout>
